<% @loan.loanees.each do |loanee|%>
  <% if loanee.user.percentage < 100 %>
  <p><%= loanee.user.first_name%><div class="progress"></p>
    <div class="progress-bar progress-bar-striped bg-warning" role="progressbar" style="width: <%= loanee.user.percentage %>%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"><%= loanee.user.percentage.round(2) %>%</div>
  </div>
  <br>
  <% else %>
    <p><%= loanee.user.first_name%><div class="progress"></p>
    <div class="progress-bar progress-bar-striped bg-success" role="progressbar" style="width: <%= loanee.user.percentage %>%" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100"><%= loanee.user.percentage.round(2) %>%</div>
  </div>
  <br>
  <% end %>
<% end %>
